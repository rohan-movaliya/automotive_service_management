<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!-- system parameter for car service duration -->
    <record id="car_service_duration" model="ir.config_parameter">
        <field name="key">automotive_service_management.service_duration</field>
        <field name="value">2</field>
    </record>

    <!-- system parameter for upi payment code -->
    <record id="payment_upi_code" model="ir.config_parameter">
        <field name="key">automotive_service_management.upi_number</field>
        <field name="value">7878787878@paytm</field>
    </record>

    <!-- system parameter for bank ifsc code -->
    <record id="payment_ifsc_code" model="ir.config_parameter">
        <field name="key">automotive_service_management.ifsc_code</field>
        <field name="value">UTBI0DMCC46</field>
    </record>

    <!-- system parameter for bank account number -->
    <record id="payment_account_number" model="ir.config_parameter">
        <field name="key">automotive_service_management.account_number</field>
        <field name="value">4111111117444490</field>
    </record>
</odoo>
